			header::after {
			  content: '';
			  display: table;
			  clear: both;
			}

			nav {
			  float: left;
				font-size: 10px;
				opacity : 1 ;

			}

			nav ul {
			  margin: 0;
			  padding: 0;
			  list-style: none;
			}

			nav li {
			  display: inline-block;
			  margin-left: 60px;
			  padding-top: 8px;
				position: relative;

			  
			}
			/* for the text you see in the nav bar */
			
			nav a {
			  color: #000000;
			  text-decoration: none;
			  text-transform: uppercase;
			  font-size: 20px;
			  
			}
						
			nav a:hover {
			  color: #000;
			}
				
			/*this code is for the black lines you see on the top of when you place your mouse on the option */
			
			nav a::before {
			  content: '';
			  display: block;
			  height: 5px;
			  background-color: #444;

			  position: absolute;
			  top: 0;
			  width: 0%;

			  transition: all ease-in-out 250ms;
			}

			nav a:hover::before {
			  width: 100%;
			}

